<div class="page-header">
  <h1 class="stores-header">Stores(<%= @stores.count %>)</h1>
</div>

<div class="container">
  <% i = 1 %>
  <% @stores.each do |store| %>
    <div class="row">
      <p><%= link_to i.to_s + ". " + store.address, "/stores/#{store.id}" %></p>
    </div>
    <% i += 1 %>
  <% end %>
</div>